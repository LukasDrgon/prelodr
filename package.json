{
  "title": "Prelodr",
  "name": "prelodr",
  "version": "2.0.0",
  "description": "A simple Material preloader inspired in Google Inbox",
  "keywords": [
    "preloader",
    "minimal",
    "responsive",
    "ui",
    "material-design"
  ],
  "license": "MIT",
  "repository": "joseluisq/prelodr",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "dependencies": {
    "emitus": "^1.0.5",
    "seqr": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "banr-stream": "^1.0.3",
    "clean-css": "^3.4.21",
    "jsdom": "^9.8.3",
    "node-sass": "3.13.0",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "main": "dist/prelodr.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "tape test.js",
    "build:css": "node-sass prelodr.scss dist/prelodr.css && cleancss dist/prelodr.css -do dist/prelodr.min.css",
    "build:js": "babel prelodr.js | banr-stream > dist/prelodr.js && cat dist/prelodr.js | uglifyjs -mc | banr-stream > dist/prelodr.min.js",
    "dist": "npm run build:css && npm run build:js",
    "pack": "webpack"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign",
      [
        "transform-es2015-modules-umd",
        {
          "globals": {
            "emitus": "Emitus",
            "seqr": "Seqr"
          },
          "exactGlobals": true
        }
      ]
    ],
    "moduleId": "Prelodr"
  }
}
